{
  "compilerOptions": {
    "lib": ["es2020", "dom"],
    "target": "es2020",
    "module": "node16",
    "moduleResolution": "node16",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["gen/ts/*.ts", "gen/typedoc/*.ts"],
  "exclude": ["node_modules"]
}
