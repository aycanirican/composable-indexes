{
  "name": "composable-indexes",
  "version": "0.0.2",
  "description": "Flexible in-memory document database-like manual index management",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "module": "commonjs",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "src/**/*.ts",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && npm run docs",
    "test": "c8 --reporter=none --verbose -- tsx --no-warnings --test-reporter spec --test src/**/*.test.ts",
    "test:watch": "tsx --no-warnings --test-reporter spec --test src/**/*.test.ts",
    "coverage": "c8 report --reporter html --reporter text --reporter lcov",
    "docs": "typedoc --json ./.docs_reflection.json --emit docs ./src/index.ts --excludeInternal",
    "prepack": "npm run build && cp ../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utdemir/composable-indexes.git"
  },
  "keywords": [
    "search",
    "db",
    "browser",
    "nodejs",
    "typescript"
  ],
  "author": "Utku Demir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/utdemir/composable-indexes/issues"
  },
  "homepage": "https://github.com/utdemir/composable-indexes#readme",
  "dependencies": {
    "long": "^5.2.3",
    "sorted-btree": "^1.8.1",
    "type-fest": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/shuffle-seed": "^1.1.0",
    "c8": "^7.14.0",
    "codecov": "^3.8.3",
    "fast-check": "^3.9.0",
    "jsdoc": "^4.0.2",
    "shuffle-seed": "^1.1.6",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  }
}
